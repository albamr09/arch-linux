{
    "nvim": {
        "theme": "'hardhackerlabs/theme-vim', { 'as': 'hardhacker' }",
        "colorscheme": "hardhacker",
        "lualine_theme": "dracula"
    },
    "dunst": {
        "font": "Blex Mono Nerd Font 9",
        "corner_radius": 1,
        "transparency": 0,
        "padding": 6,
        "horizontal_padding": 6,
        "separator_color": "frame",
        "frame_width": 1,
        "frame_color": "#8EC07C",
        "low_frame_color": "#285577",
        "low_foreground": "#b8bec6",
        "low_background": "#011627",
        "normal_frame_color": "#285577",
        "normal_foreground": "#b8bec6",
        "normal_background": "#011627",
        "critical_frame_color": "#B7472A",
        "critical_foreground": "#b8bec6",
        "critical_background": "#011627"
    },
    "picom": {
        "frame_opacity": 1.0
    },
    "alacritty": {
        "bright_black": "0x3f3951",
        "bright_blue": "0xb1baf4",
        "bright_cyan": "0xb3f4f3",
        "bright_green": "0xb1f2a7",
        "bright_magenta": "0xe192ef",
        "bright_red": "0xe965a5",
        "bright_white": "0xeee9fc",
        "bright_yellow": "0xebde76",
        "normal_black": "0x282433",
        "normal_blue": "0xb1baf4",
        "normal_cyan": "0xb3f4f3",
        "normal_green": "0xb1f2a7",
        "normal_magenta": "0xe192ef",
        "normal_red": "0xe965a5",
        "normal_white": "0xeee9fc",
        "normal_yellow": "0xebde76",
        "primary_background": "0x282433",
        "primary_foreground": "0xeee9fc",
        "font_size": 10.0,
        "font_bold": "JetBrainsMono Nerd Font",
        "font_italic": "JetBrainsMono Nerd Font",
        "font_normal": "JetBrainsMono Nerd Font",
        "window_padding": 13
    },
    "tmux": {
        "theme": "source-file ~/.config/tmux/dracula.tmux"
    },
    "i3": {
        "font": "JetBrainsMono 9",
        "gaps_inner": 4,
        "gaps_outer": 1
    },
    "qutebrowser": {
        "theme": "config.source('themes/hardharcker.py')"
    },
    "i3lock": {
        "theme": "$HOME/.config/i3lock/default/lock.sh"
    },
    "zsh": {
        "theme": "pi"
    },
    "lightdm": {
        "font": "JetBrainsMonoMedium NF Medium 10"
    }
}