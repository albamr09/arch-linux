{
    "nvim": {
        "theme": "'kinoute/vim-hivacruz-theme'",
        "colorscheme": "hivacruz",
        "lualine_theme": "solarized_dark"
    },
    "dunst": {
        "font": "Blex Mono Nerd Font 9",
        "corner_radius": 1,
        "transparency": 0,
        "padding": 6,
        "horizontal_padding": 6,
        "separator_color": "frame",
        "frame_width": 1,
        "frame_color": "#8EC07C",
        "low_frame_color": "#285577",
        "low_foreground": "#b8bec6",
        "low_background": "#011627",
        "normal_frame_color": "#285577",
        "normal_foreground": "#b8bec6",
        "normal_background": "#011627",
        "critical_frame_color": "#B7472A",
        "critical_foreground": "#b8bec6",
        "critical_background": "#011627"
    },
    "picom": {
        "frame_opacity": 0.7
    },
    "alacritty": {
        "bright_black": "#6b7394",
        "bright_blue": "#898ea4",
        "bright_cyan": "#9c637a",
        "bright_green": "#73ad43",
        "bright_magenta": "#dfe2f1",
        "bright_red": "#c76b29",
        "bright_white": "#f5f7ff",
        "bright_yellow": "#5e6687",
        "normal_black": "#202746",
        "normal_blue": "#3d8fd1",
        "normal_cyan": "#22a2c9",
        "normal_green": "#ac9739",
        "normal_magenta": "#6679cc",
        "normal_red": "#c94922",
        "normal_white": "#979db4",
        "normal_yellow": "#c08b30",
        "primary_background": "#132638",
        "primary_foreground": "#ede4e4",
        "font_size": 9.0,
        "font_bold": "JetBrainsMono Nerd Font Mono",
        "font_italic": "JetBrainsMono Nerd Font Mono",
        "font_normal": "JetBrainsMono Nerd Font Mono",
        "window_padding": 10
    },
    "i3": {
        "font": "JetBrainsMono 8",
        "gaps_inner": 4,
        "gaps_outer": 1
    },
    "i3lock": {
        "theme": "$HOME/.config/i3lock/default/lock.sh"
    },
    "zsh": {
        "theme": "robbyrussell"
    },
    "lightdm": {
        "font": "JetBrainsMonoMedium NF Medium 10"
    }
}