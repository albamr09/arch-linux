{
    "nvim": {
        "theme": "'EdenEast/nightfox.nvim'",
        "colorscheme": "nightfox",
        "lualine_theme": "palenight"
    },
    "dunst": {
        "font": "JetBrains Mono NL 10",
        "corner_radius": 2,
        "transparency": 5,
        "padding": 10,
        "horizontal_padding": 10,
        "separator_color": "#000",
        "frame_width": 1,
        "frame_color": "#8EC07C",
        "low_frame_color": "#201723",
        "low_foreground": "#47474f",
        "low_background": "#d8d8c7",
        "normal_frame_color": "#201723",
        "normal_foreground": "#47474f",
        "normal_background": "#d8d8c7",
        "critical_frame_color": "#201723",
        "critical_foreground": "#ba1a0a",
        "critical_background": "#d8d8c7"
    },
    "picom": {
        "frame_opacity": 1.0
    },
    "alacritty": {
        "bright_black": "#dfe8dc",
        "bright_blue": "#84e8c1",
        "bright_cyan": "#ffbf59",
        "bright_green": "#59ff6c",
        "bright_magenta": "#7843d3",
        "bright_red": "#ff6c59",
        "bright_white": "#575c65",
        "bright_yellow": "#ebff59",
        "normal_black": "#dfe8dc",
        "normal_blue": "#aee5d0",
        "normal_cyan": "#f9d498",
        "normal_green": "#98f9a3",
        "normal_magenta": "#b393ea",
        "normal_red": "#f9a398",
        "normal_white": "#575c65",
        "normal_yellow": "#eef998",
        "primary_background": "0x192330",
        "primary_foreground": "#cfc4c3",
        "font_size": 11,
        "font_bold": "JetBrains Mono NL",
        "font_italic": "JetBrains Mono NL",
        "font_normal": "JetBrains Mono NL",
        "window_padding": 10
    },
    "tmux": {
        "theme": "source-file ~/.config/tmux/nightfox.tmux"
    },
    "rofi": {
        "theme": "themes/light"
    },
    "i3": {
        "font": "JetBrains Mono NL 10",
        "gaps_inner": 5,
        "gaps_outer": 5
    },
    "qutebrowser": {
        "theme": "config.source('themes/nord-qutebrowser.py')"
    },
    "i3lock": {
        "theme": "$HOME/.config/i3lock/light/lock.sh"
    },
    "zsh": {
        "theme": "typewritten",
        "theme_configuration": "TYPEWRITTEN_PROMPT_LAYOUT=\"pure\"\nTYPEWRITTEN_SYMBOL=\" ➜ \"\nTYPEWRITTEN_ARROW_SYMBOL=\" ➜ \"\nZSH_AUTOSUGGEST_HIGHLIGHT_STYLE=\"fg=#465061\""
    },
    "lightdm": {
        "font": "JetBrains Mono NL 10"
    }
}